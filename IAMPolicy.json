{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "Statement1",
			"Effect": "Allow",
			"Action": [
				"ec2:DescribeInstances"
			],
			"Resource": [
				"arn:aws:ec2:<region>:<account-id>:instance*"
			]
		},
		{
			"Sid": "Statement2",
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:PutObject"
			],
			"Resource": [
				"arn:aws:s3:::<bucket-name>:<object-key>"
			]
		},
		{
			"Sid": "Statement3",
			"Effect": "Allow",
			"Action": [
				"logs:*"
			],
			"Resource": [
				"*"
			]
		}
	]
}
